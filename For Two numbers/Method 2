#include <stdio.h>
int main()
{
    int i,a,b,ans=0;
    printf ("Enter two numbers: ");
    scanf ("%d %d",&a,&b);
    i=b;
    while (i!=0)
    {
        if (i%a==0 && i%b==0)
        {
            ans=i;
            break;
        }
        i++;
    }
    printf ("The LCM of %d and %d is= %d",a,b,ans);
    return 0;
}
